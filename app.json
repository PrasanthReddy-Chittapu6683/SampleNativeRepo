{
  "expo": {
    "name": "EDNA App",
    "slug": "edna",
    "version": "1.0.84",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#12121A",
    "splash": {
      "backgroundColor": "#12121A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.motivepartners.edna",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for certain activities.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library for certain activities.",
        "NSPhotoLibraryAddUsageDescription": "Save pictures of files for certain activities.",
        "LSApplicationQueriesSchemes": [
          "mailto",
          "message",
          "ms-outlook",
          "googlegmail",
          "yandexmail",
          "ymail"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0495a169-2af0-4034-8ffc-8d12820661cf"
      },
      "bugsnag": {
        "apiKey": "53c0df7a64bd10a2a1cd07df12e3f6d9"
      }
    },
    "plugins": [
      "expo-av",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "react-native-email-link",
      "@bugsnag/plugin-expo-eas-sourcemaps"
    ]
  }
}
